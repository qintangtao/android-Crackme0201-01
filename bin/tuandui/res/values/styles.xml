<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="bdp_update_dialog_style" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Translucent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="bdp_update_dialog_style_fullscreen" parent="@style/bdp_update_dialog_style">
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="bdp_update_progress_download">
        <item name="android:maxHeight">8.0dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/bdp_update_progress_download</item>
        <item name="android:minHeight">8.0dip</item>
    </style>
    <style name="AppTheme" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/tran_black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
    </style>
    <style name="activityAnimation" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/slide_right_in</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_left_in</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_right_out</item>
    </style>
    <style name="app_theme" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
    </style>
    <style name="main_theme" parent="@style/app_theme">
        <item name="android:windowBackground">@drawable/start_image</item>
    </style>
    <style name="show_image_anim" parent="@android:style/Theme">
        <item name="android:windowBackground">@color/tran_black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@style/image_anim</item>
    </style>
    <style name="image_anim" parent="@android:style/Animation.Activity">
        <item name="android:activityOpenEnterAnimation">@anim/group_enter_in</item>
        <item name="android:activityOpenExitAnimation">@anim/group_enter_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/group_back_in</item>
        <item name="android:activityCloseExitAnimation">@anim/group_back_out</item>
    </style>
    <style name="MyScrollViewStyle">
        <item name="android:scrollbars">none</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="words_shadow">
        <item name="android:shadowColor">#ff1e436c</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">-1.5</item>
        <item name="android:shadowRadius">0.1</item>
    </style>
    <style name="bottomStyle">
        <item name="android:windowEnterAnimation">@anim/bottom_pup_anim_out</item>
        <item name="android:windowExitAnimation">@anim/bottom_pup_anim_in</item>
    </style>
    <style name="line_hori">
        <item name="android:background">@color/line</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.6dip</item>
    </style>
    <style name="custom_checkbox_bulksms" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_sel_bulksms</item>
    </style>
    <style name="custom_checkbox_setting" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_sel_setting</item>
    </style>
    <style name="StyleTextViewSetting">
        <item name="android:textSize">@dimen/text_view_label</item>
        <item name="android:textColor">#ff000000</item>
    </style>
    <style name="dialogWindowAnim" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit_anim</item>
    </style>
    <style name="StyleTextView">
        <item name="android:textSize">@dimen/contact_textview_textsize</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ff001188</item>
    </style>
    <style name="StyleTextViewSettingSmall">
        <item name="android:textSize">@dimen/text_view_summary</item>
        <item name="android:textColor">#ff546483</item>
    </style>
    <style name="CustomProgressStyle" parent="@android:style/Widget.ProgressBar.Large">
        <item name="android:maxWidth">100.0dip</item>
        <item name="android:maxHeight">100.0dip</item>
        <item name="android:minWidth">100.0dip</item>
        <item name="android:minHeight">100.0dip</item>
    </style>
    <style name="customProgressBar">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">40.0dip</item>
        <item name="android:layout_height">40.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/loading_0</item>
    </style>
    <style name="weekName">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@color/schedule_week</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="my_listview_style">
        <item name="android:fadingEdge">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:listSelector">#00000000</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="my_lv_style">
        <item name="android:fadingEdge">none</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:listSelector">#00000000</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="AnimTools" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/turn_in</item>
        <item name="android:windowExitAnimation">@anim/turn_out</item>
    </style>
    <style name="Ap_Pop_Style" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/ap_pop_in</item>
        <item name="android:windowExitAnimation">@anim/alpha_out</item>
    </style>
    <style name="umeng_socialize_action_bar_itemlayout">
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">4.0dip</item>
        <item name="android:paddingTop">4.0dip</item>
        <item name="android:paddingRight">4.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="umeng_socialize_action_bar_item_im">
        <item name="android:layout_width">25.0dip</item>
        <item name="android:layout_height">25.0dip</item>
        <item name="android:scaleType">fitCenter</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="umeng_socialize_action_bar_item_tv">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">5.0dip</item>
        <item name="android:text">999</item>
    </style>
    <style name="umeng_socialize_popup_dialog_anim" parent="@style/umeng_socialize_popup_dialog">
        <item name="android:windowAnimationStyle">@style/umeng_socialize_dialog_animations</item>
    </style>
    <style name="umeng_socialize_popup_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="umeng_socialize_dialog_animations">
        <item name="android:windowEnterAnimation">@anim/umeng_socialize_slide_in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/umeng_socialize_slide_out_from_bottom</item>
    </style>
    <style name="umeng_socialize_dialog_anim_fade">
        <item name="android:windowEnterAnimation">@anim/umeng_socialize_fade_in</item>
        <item name="android:windowExitAnimation">@anim/umeng_socialize_fade_out</item>
    </style>
    <style name="umeng_socialize_shareboard_animation">
        <item name="android:windowEnterAnimation">@anim/umeng_socialize_shareboard_animation_in</item>
        <item name="android:windowExitAnimation">@anim/umeng_socialize_shareboard_animation_out</item>
    </style>
    <style name="Theme.UMDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Theme.UMDefault" parent="@android:style/Theme.NoTitleBar" />
    <style name="umeng_socialize_divider">
        <item name="android:background">@color/umeng_socialize_divider</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
    </style>
    <style name="umeng_socialize_list_item">
        <item name="android:background">@color/umeng_socialize_list_item_bgcolor</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">55.0dip</item>
    </style>
    <style name="umeng_socialize_edit_padding">
        <item name="android:background">@color/umeng_socialize_list_item_bgcolor</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">6.0dip</item>
        <item name="android:layout_marginTop">6.0dip</item>
        <item name="android:layout_marginRight">6.0dip</item>
        <item name="android:layout_marginBottom">6.0dip</item>
    </style>
</resources>
